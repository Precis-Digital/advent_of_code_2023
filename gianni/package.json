{
  "name": "aoc2023",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "",
  "scripts": {
    "start": "for dir in src/days/*/; do node ${dir}index.js; done",
    "start:min": "npm run -s minify && for dir in src/days/*/; do node ${dir}index.min.js; done",
    "start:min:day": "npm run -s minify && node -e 'require(`child_process`).execSync(`node ./src/days/day${process.argv[1]}/index.min.js`, { stdio: [0, 1, 2] })'",
    "start:day": "node -e 'require(`child_process`).execSync(`node ./src/days/day${process.argv[1]}/index.js`, { stdio: [0, 1, 2] })'",
    "minify": "for file in $(find ./src -name '*.js' ! -name '*.min.js'); do uglifyjs $file -c drop_console=true -m --no-annotations --toplevel -o ${file%.js}.min.js; sed -i '' -e 's/class.js/class.min.js/g' ${file%.js}.min.js; sed -i '' -e 's/utils.js/utils.min.js/g' ${file%.js}.min.js; done",
    "docs": "jsdoc -c jsdoc.json -t ./node_modules/better-docs"
  },
  "author": "Gianni Marsiglia",
  "license": "ISC",
  "dependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "better-docs": "^2.7.2",
    "chalk": "^5.3.0",
    "cross-var": "^1.1.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-promise": "^5.1.0",
    "jsdoc": "^4.0.2",
    "jsdoc3-bootstrap": "^0.15.0",
    "minify": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "taffydb": "^2.7.3",
    "uglify-js": "^3.17.4"
  }
}
